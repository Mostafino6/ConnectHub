<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.demo.Controller">
    <children>
        <Label layoutX="10.0" layoutY="10.0" styleClass="header" stylesheets="@main.css" text="Stories" />

        <ScrollPane layoutX="10.0" layoutY="50.0" prefHeight="340.0" prefWidth="580.0">
            <content>
                <VBox fx:id="storyContainer" spacing="10.0" styleClass="storyContainer">
                </VBox>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
